.win{
  background: #fef3bb;
  .win-content{
    width:1275px;
    margin: 0 auto;
    .top-cont{
      width:1129px;
      height:672px;
      background:url(../../../img/wuyaoba.png) no-repeat;
      margin-left:62px;
    }
    .center-cont{
      width:865px;
      margin:0 auto;
      text-align:left;
      .top{
        font-size:30px;
        margin-top:50px;
      }
      .item1{
        font-size:16px;
        line-height: 28px;
      }
    }
    .bottom-cont{
      background: #fee36d;
      border-radius: 40px;
      width:865px;
      margin:50px auto 0;
      padding: 30px;
    }
    .caption{
      margin-top: 20px;
       .item{
         .num{
          width:35px;
          height:35px;
          font-size:20px;
          text-align:center;
          line-height:35px;
          color:#fff;
          display:inline-block;
          border-radius:50%;
          background:#e73d38;
         }
         .prayer{
          font-size:24px;
          color:#ec1a13;
          margin-left: 15px;
         }
        }
        .cont{
          font-size:18px;
          margin-left:50px;
          line-height: 26px;
          color:#666;
        }
        .zhongjiang{
          font-size:24px;
          color:#ec1a13;
          margin: 20px 0 0 47px;
        }
     }
     .footer{
      font-size:18px;
      color:#000;
      margin:20px 0 0 47px;
     }
    }
    .lingtouniao{
      width: 865px;
      margin:20px auto 0;
      text-align:right;
      line-height:25px;
      font-size:18px;
    }
    .footer-img{
      height:256px;
      img{
        width:100%;
      }
    }
}/* win */