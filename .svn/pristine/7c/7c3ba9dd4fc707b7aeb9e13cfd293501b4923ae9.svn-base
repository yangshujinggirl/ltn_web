.mutual_control{
  .mutual_control_cont{
    padding:10px 35px 35px;
    .itemnav {
       font-size: 14px;
       font-weight: 700;
        height: 50px;
       border-bottom: 1px solid #e2e2e2;
       .itemnav-left {
          text-align: center;
          float: left;
          .ite {
            width: 100px;
            height: 50px;
            display: inline-block;
            line-height: 60px;
            color: #333;
            position: relative;
            bottom:-1px;
            &.selected {
              border-bottom: 3px solid #ff5900;
              color: #ff5900;
            }
          }
       }
       .itemnv-right {
        float: right;
        font-size: 14px;
        padding-left: 124px;
        height: 50px;
        color: #999;
        line-height: 60px;
         background:url(/img/viewall_liandong.png) no-repeat left center;
       }
    }
    .mutual_contne_bottom{
      padding: 0 10px;
      .caption{
        width: 838px;
        height: 117px;
        border: 1px solid #e2e2e2;
        margin-top: 35px;
        border-radius: 5px;
        padding: 28px;
        position: relative;
        .mutual_control_title{
          font-size: 14px;
          color: #b0b3b4;
        }
        .mutual_control_prop{
          margin-top: 25px;
          .control_prop{
            display: inline-block;
            .item{
              font-size: 26px;
              color: #394043;
              .renew{
                width: 48px;
                height: 20px;
                display: inline-block;
                border-radius: 10px;
                border: 1px solid #e2e2e2;
                margin-left: 10px;
                font-size: 14px;
                color: #888c8e;
                line-height: 20px;
                text-align: center;
                position: relative;
                top: -5px;
              }
            }
            .filed{
              font-size: 14px;
              color: #888c8e;
            }
          }
          .control_prop2{
            margin-left: 133px;
          }
        }
        .mutual_icon{
          width: 67px;
          height: 57px;
          font-size: 12px;
          color: #fff;
          background: url(/img/imageTwo/mutual/crowd_guancha.png) no-repeat;
          position: absolute;
          right: 40px;
          top: 0;
          text-align: center;
          padding-top: 8px;
        }
      }
      .nolist {
        display: none;
        width: 196px;
        height: 179px;
        margin: 0 auto;
        text-align: center;
        position: relative;
        background: url("/img/jz.jpg") no-repeat;
        margin-top: 40px;
        .logoIcon {
            position: absolute;
            left: -22px;
            top: 130px;
            font-size: 16px;
            .denglu {
                color: #ff5a00;
            }
            .denglu:hover {
                color: #6e56ff;
            }
        }
      }
    }
  }
}