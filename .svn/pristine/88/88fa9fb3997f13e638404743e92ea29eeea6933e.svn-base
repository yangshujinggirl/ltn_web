@charset "utf-8";
 .socialShare {
    display: inline-block;
    width: 54px;
    height: 54px;
    position: relative;
    top: 34px;
    left: 70px;
    z-index: 1;
}
.socialShare .msb_main {
    display: block;
    width: 46px;
    height: 46px;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 2;
    cursor: pointer;
    text-indent: -9999px;
    border: 4px solid #FFF;
    box-shadow: 0 0 5px #DDD;
    border-radius: 36px;
}
.socialShare .msb_main:hover {
    box-shadow: 0 0 5px #BBB;
}
.socialShare .msb_main img {
    width: 46px;
    height: 46px;
    cursor: pointer;
    border-radius: 23px;
    border: none;
    float: left
}
.socialShare .msb_network_button {
    width: 64px;
    height: 65px;
    position: absolute;
    top: -5px;
    left: 92px;
    z-index: 1;
    cursor: pointer;
    text-indent: -9999px;
    background: no-repeat;
}
.socialShare .msb_network_button.sina {
    margin-left: -3px;
    background: url(/html/img/restful_icon4.png) no-repeat;
}
.socialShare .msb_network_button.tQQ {
    margin-left: 216px;
    background: url(/html/img/restful_icon6.png) no-repeat;
}
.socialShare .msb_network_button.qZone {
    margin-left: 107px;
    background: url(/html/img/restful_icon5.png) no-repeat;
}
.socialShare .msb_network_button.weixin {
    margin-left: -111px;
    background: url(/html/img/restful_icon3.png) no-repeat;
}
