.account-detailmoney .detailmoney-content {
  $high:#ff5900;
  padding:10px 35px 35px ;
  .itemnav {
     font-size: 14px;
     font-weight: 700;
      height: 50px;
     border-bottom: 1px solid #e2e2e2;
     .itemnav-left {
        text-align: center;
        float: left;
        .ite {
          width: 100px;
          height: 50px;
          display: inline-block;
          line-height: 60px;
          color: #333;
          position: relative;
          bottom:-1px;
          &.selected {
            border-bottom: 3px solid $high;
            color: $high;
          }
        }
     }
     .itemnv-right {
      float: right;
      font-size: 14px;
      padding-left: 124px;
      height: 50px;
      color: #999;
      line-height: 60px;
       background:url(/img/viewall_liandong.png) no-repeat left center;
     }
  }
  .wrapOne {
    padding:0 10px;
  }
  .headeramcount {
    font-size: 14px;
    overflow: hidden;
    zoom:1;
    background-color: #f7f7f7;
    padding:10px 0;
    margin:30px 0;
    .same {
      padding:38px 0;
      padding-left: 50px;
    }
    .remain {
      text-align: center;
      float: left;
      .useble {
        font-size: 22px;
        color: #999;
      }
      .btns {
        padding-top: 42px;
        .ite {
          width: 90px;
          display: inline-block;
          margin: 0 4px;
          border-radius: 5px;
          height: 30px;
          line-height: 30px;
        }
        .ain {
          background-color: $high;
          color: #fff;
        }
        .aout {
          border: 1px solid $high;
          color: $high;
        }
        .ainvest {
          border: 1px solid #2079ff;
          color: #2979ff;
        }
      }
      .value {
        color: #3a3a3a;
        font-size: 36px;
      }
    }
    .sum {
      float: right;
      width: 50%;
      border-left:1px solid #e2e2e2;
      .item {
        .info {
          font-size:14px;
          color: #666;
        }
        .value {
          font-size:18px;
          color: #333;
        }
        &:not(:first-child) {
          margin-top: 37px;
        }
      }
    }
  }

  .tipsinfo {
    height: 32px;
    line-height: 32px;
    background:url(/img/trackRecord_ico1.png) no-repeat left center;
    padding-left: 40px;
    mark {
     background-color:#f7f7f7;
    }
    margin-bottom: 40px;
  }
  .form {
    position: relative;
    .row {
     font-size: 14px;
     margin: 25px 0 0;
     input {
       margin: 0 5px;
     }
     .type {
       display: inline-block;
       padding-left: 28px;
       .item {
          display: inline-block;
          width: 67px;
          height: 30px;
          border:1px solid  #fff;
          line-height: 30px;
          cursor: pointer;
          text-align: center;
         &:hover {
           border:1px solid  $high;
           color: $high;
           border-radius: 30px;
         }
         &.selected {
           border:1px solid  $high;
           color: $high;
           border-radius: 30px;
         }
       }
     }
     .dateWrap {
        display: inline-block;
        vertical-align: middle;
        .inline {
          width:150px;
          height: 22px;
          line-height: 22px;
          width: 150px;
          margin: 0;
          border: 1px solid #ccc;
        }
      }
    }
  }
  table {
   width: 100%;
   text-align: left;
   margin-top: 20px;
    thead {
      tr {
        background-color: #ebebeb;
        th {
          font-size: 14px;
          padding: 12px 20px;
          color: #3a3a3a;
        }
      }
    }
    tbody {
      vertical-align: top;
      font-size: 12px;
      color: #999;
      tr {
        border-bottom: 1px dashed #e2e2e2;
       td {
        padding: 20px 20px;
       }
      }
      .dealtime {
        width: 70px;
      }
      .red {
        color:#fc613b;
      }
      .green {
        color:#04c004;
      }
    }
  }
  .nolist {
    display: none;
    width: 196px;
    height: 179px;
    margin: 0 auto;
    background: url(/img/jz.jpg);
  }
}