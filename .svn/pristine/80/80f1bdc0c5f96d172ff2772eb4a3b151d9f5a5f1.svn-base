    .leftBtn,.rightBtn {
      width:15px;
      height: 29px;
      position:absolute;
      top:50%;
      transform: translateY(-50%);
      -webkit-transform: translateY(-50%);
      cursor: pointer;
    }
    .leftBtn {
      background: url('/img/imageTwo/icon/icon_homeLeft.png') no-repeat center;
      left:0;
    }
    .rightBtn {
      background: url('/img/imageTwo/icon/icon_homeRight.png') no-repeat center;
      right: 0;
    }
  }
  .leftNav {
    position:fixed;
    right:0;
    top:50%;
    transform:translateY(-50%);
    -webkit-transform:translateY(-50%);
    z-index: 99999999;
    .navList {
      .navItem {
        width: 34px;
        height: 34px;
        line-height: 34px;
        cursor: pointer;
        position:relative;
        .top {
          width: 34px;
          height: 34px;
          background-color: #434343;
          position:absolute;
          top:0;
          right: 0;
          z-index: 33;
        }
        .hover {
          width: 126px;
          background-color: $high;
          border-radius:4px;
          color: #fff;
          position:absolute;
          right: -126px;
          top:0;
          z-index: 22;
          .text {
            display: block;
            padding-left: 18px;
            height: 34px;
          }
          .codeImg {
            padding-left:2px;
          }
        }
        &:not(:first-child) {
          margin-top: 4px;
        }
        &:hover {
            .top {
              background-color: $high;
            }
        }
      }
      .money .hover{
        .text {
          padding-left: 0;
        }
      }
      .money {
        .top {
          background: #434343 url('/img/imageTwo/home/icon_homeleft5.png') center no-repeat;
        }
      }
      .phone {
        .top {
          background: #434343 url('/img/imageTwo/home/icon_homeleft2.png') center no-repeat;
        }
      }
      .weixin {
        .top {
          background: #434343 url('/img/imageTwo/home/icon_homeleft3.png') center no-repeat;
        }
      }
      .returnTop {
        display: none;
        .top {
          background: #434343 url('/img/imageTwo/home/icon_homeleft4.png') center no-repeat;
        }
      }
    }
  }
}
