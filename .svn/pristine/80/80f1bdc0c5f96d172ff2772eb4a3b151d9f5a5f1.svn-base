$home-banner-height: 360px;

.homeIndex {
  .swiper-container{
    width: 100%;
    height: 100%;
  }
  p,h1,ul,li {
    margin:0;
    padding:0;
  }
  .wrap {
    padding:0;
  }
  $high:#ff6f20;
  .banner {
    position: relative;
    .wrapOne {
      width:1190px;
      position:absolute;
      left:50%;
      transform:translateX(-50%);
      top:0;
      z-index: 99;
    }
    .swiper-slide{
      overflow: hidden;
        a{
          width: 100%;
          height: 500px;
          position: relative;
          display: inline-block;
          img{
            position: absolute;
            left: 50%;
            top: 0;
            transform: translateX(-50%);
            -webkit-transform: translateX(-50%);
          }
        }
    }
    .entrance {
      width: 340px;
      height: 254px;
      background:rgba(0,0,0,.2);
      position:absolute;
      top:96px;
      right: 0;
      .btn {
        width: 248px;
        height: 40px;
        border: 1px solid $high;
        color: $high;
        line-height: 40px;
        background:none;
        font-size: 18px;
        margin-bottom:18px;
        &:hover{
          color: #fff;
          background: $high;
        }
      }
      .tt {
        font-size: 12px;
        color: #fff;
      }
      .register {
        text-align: center;
        display: none;
        .head {
          font-size: 20px;
          font-weight: normal;
          color: #fff;
          padding: 30px 0 10px;
          margin:0;
        }
        .percent {
          font-size: 22px;
          color: $high;
          .big {
            font-weight: bold;
            font-size: 76px;
          }
        }
        &.selected {
          display: block;
        }
      }
      .goAccount {
        font-size: 14px;
        color: #fff;
        padding-left: 47px;
        display: none;
        .property {
          padding:30px 0 20px;
          .redbig {
            font-size: 44px;
            color: $high;
          }
        }
        .difference {
          margin-bottom: 10px;
          .big {
            font-size: 20px;
          }
        }
        &.selected {
          display: block;
        }
      }
    }
  }
  .dateList {
    padding:16px 0;
    font-size: 0;
    -webkit-text-size-adjust:none;
    .dateItem {
      display: inline-block;
      font-size: 14px;
      color: #888c8e;
      width:25%;
      box-sizing:border-box;
      -webkit-box-sizing:border-box;
      text-align:center;
      .num {
        font-size: 26px;
        color: #394043;
        .unite {
          font-size: 14px;
        }
      }
    }
    .dateItem:not(:last-child){
      border-right: 1px dashed #e1e1e1;
    }
    .report {
      background: url('/img/imageTwo/home/icon_home5.png') no-repeat 30px center;
      .link {
        color: #888c8e;
        display: block;
        padding-top:8px;
      }
      &:hover {
        background: url('/img/imageTwo/home/icon_home55.png') no-repeat 30px center;
        .link {
          color: $high;
        }
      }
    }
  }
  .partOne {
    background-color: #FFF;
    .wrap {
      overflow: visible;
    }
    .list {
      font-size: 0;
      -webkit-text-size-adjust:none;
    }
    .partItem {
      width: 25%;
      height: 200px;
      display: inline-block;
      text-align: center;
      font-size: 18px;
      color: #394043;
      padding-top:44px;
      box-sizing:border-box;
      -webkit-box-sizing:border-box;
      cursor: pointer;
      .icon {
        display: inline-block;
        width: 54px;
        height: 54px;
      }
      .little {
        font-size: 12px;
        color: #888c8e;
        padding-top:10px;
      }
      &:hover {
        box-shadow:0 0 10px rgba(0,0,0,.2);
      }
    }
    .moreLine {
      .icon {
        background: url('/img/imageTwo/home/icon_home10.png') no-repeat center;
      }
    }
    .safe {
      .icon {
        background: url('/img/imageTwo/home/icon_home6.png') no-repeat center;
      }
    }
    .deal {
      .icon {
        background: url('/img/imageTwo/home/icon_home7.png') no-repeat center;
      }
    }
    .short {
      .icon {
        background: url('/img/imageTwo/home/icon_home8.png') no-repeat center;
      }
    }
  }
  .topNav {
    overflow: hidden;
    zoom:1;
    margin: 36px 0 26px;
    .left {
      font-size: 30px;
      color: #676d70;
      .tall {
        font-weight: bold;
        color: #383f44;
      }
    }
    .right {
      line-height: 40px;
      a {
        font-size: 14px;
        color:#288bff;
      }
    }
  }
  .partTwo {
    margin-bottom: 52px;
    .wrap {
      overflow: visible;
    }
    .list {
      padding: 0;
      font-size: 0;
      -webkit-text-size-adjust: none;
      .item {
        height: 248px;
        display: inline-block;
        width: 33%;
        text-align:center;
        vertical-align: top;
        .wp {
          height: 100%;
          padding:56px 0 0;
          background-color: #eeeeee;
          box-sizing:border-box;
          -webkit-box-sizing:border-box;
          .title {
            display: inline-block;
            font-size: 36px;
            color: #666;
          }
          .txt {
            font-size: 22px;
            color:$high;
          }
          .icon {
            display: inline-block;
            width:59px;
            height: 44px;
            background: url('/img/imageTwo/home/icon_home9.png') no-repeat center;
          }
        }
        .wp2 {
          height: 100%;
          background-color: #FFF;
          padding:30px 80px 0;
          box-sizing:border-box;
          -webkit-box-sizing:border-box;
          color: #888c8e;
          cursor: pointer;
          .title {
            font-size: 14px;
            color:#b0b3b4;
            .little {
              font-size: 12px;
              padding-left:10px;
            }
          }
          .middle {
            font-size: 12px;
            .num {
              color: #394043;
              .big {
                font-size: 60px;
              }
              .little {
                font-size: 32px;
              }
            }
          }
          .bottom {
            font-size: 14px;
            overflow: hidden;
            zoom:1;
            padding:20px 0;
            .bold {
              font-weight: bold;
              color: #333;
            }
          }
          .btn {
            padding: 0;
            width: 108px;
            height: 30px;
            font-size: 14px;
            text-align:center;
            line-height: 30px;
            border: 1px solid $high;
            background:none;
            color: $high;
            border-radius: 2px;
            -webkit-border-radius: 2px;
          }
        }
      }
      .item:not(:first-child) {
        &:hover {
            position:relative;
            box-shadow: 0 0 10px rgba(0,0,0,.2);
            .num {
              color:$high;
            }
            .btn {
              background-color: $high;
              color: #fff;
            }
        }
      }
    }
  }
  .partThr {
    background-color: #FFF;
    padding-bottom:20px;
    .tapWrap {
      width: 100%;
      position:relative;
      cursor: pointer;
    }
    .list {
      height: 400px;
      position:relative;
      .info {
        width: 400px;
        padding: 0 20px 80px;
        position:absolute;
        right: 60px;
        top:50%;
        transform:translateY(-50%);
        -webkit-transform:translateY(-50%);
          .infoTitle{
            font-size: 44px;
            color: #394043;
          }
          .contentOne {
            font-size: 24px;
            color: #394043;
            margin:10px 0 28px;
          }
          .contentTwo {
            line-height: 24px;
            font-size: 14px;
            color: #888c8e;
          }
        .btn {
          padding: 0;
          width: 136px;
          height: 40px;
          font-size: 18px;
          text-align: center;
          line-height: 40px;
          background:none;
          color: $high;
          border: 1px solid $high;
          position:absolute;
          left: 20px;
          bottom:0;
          border-radius: 2px;
          -webkit-border-radius: 2px;
          &:hover {
            background-color: $high;
            color:#fff;
          }
        }
      }
    }
    .leftBtn,.rightBtn {
      width: 56px;
      height: 56px;
      position:absolute;
      top:50%;
      transform: translateY(-50%);
      cursor: pointer;
      z-index: 999;
      display: none;
    }
    .leftBtn {
      background: url('/img/imageTwo/icon/icon_home10.png') no-repeat center;
      left:10px;
      &:hover {
        background: url('/img/imageTwo/icon/icon_home1010.png') no-repeat center;
      }
    }
    .rightBtn {
      background: url('/img/imageTwo/icon/icon_home11.png') no-repeat center;
      right:10px;
      &:hover {
        background: url('/img/imageTwo/icon/icon_home1111.png') no-repeat center;
      }
    }
  }
  .partFour {
    margin-bottom: 43px;
    .list {
      overflow: hidden;
      zoom:1;
      .item {
        float:left;
        width:570px;
        .bgwp {
          width: 100%;
          height: 298px;
          overflow: hidden;
          .bg {
            width:100%;
          }
        }
        .content {
          height: 202px;
          padding:30px 35px 30px;
          background-color: #FFF;
          box-sizing:border-box;
          -webkit-box-sizing:border-box;
          .name {
            font-size: 22px;
            font-weight: bold;
            color: #394043;
          }
          .intro {
            color: #b0b3b4;
            padding-top:12px;
          }
          .process {
            width:100%;
            height: 4px;
            background-color: #ccc;
            position:relative;
            margin:20px 0 10px;
            .inner {
              width:20%;
              height: 4px;
              background:linear-gradient( left,#ffcc00,#fc7800);
              background:-o-linear-gradient( left,#ffcc00,#fc7800);
              background:-webkit-linear-gradient( left,#ffcc00,#fc7800);
              background:-moz-linear-gradient( left,#ffcc00,#fc7800);
              position:absolute;
              left:0;
              top:0;
            }
          }
          .join {
            color: #888c8e;
            overflow: hidden;
            zoom:1;
            .bold {
              font-size: 20px;
              color: #394043;
            }
            .target,.project,.btn {
              float: left;
              line-height:30px;
            }
            .project {
              margin-left: 98px;
            }
            .btn {
              width: 108px;
              height: 30px;
              line-height: 30px;
              font-size: 14px;
              color: $high;
              border: 1px solid $high;
              background:none;
              float: right;
              border-radius: 2px;
              -webkit-border-radius: 2px;
              &:hover {
                background-color: $high;
                color: #fff;
              }
            }
          }
        }
      }
      .item:last-child{
        float: right;
      }
      .item-no {
        width:570px;
        float: right;
      }
    }
  }
  .partFive {
    padding-bottom: 50px;
    background-color: #fff;
    .wrap {
     /* overflow: visible;*/
    }
    .productContent {
      position:relative;
      border: 1px solid #ccc;
      box-sizing:border-box;
      -webkit-box-sizing:border-box;
      .leftContent {
        width:300px;
        height: 100%;
        background:url('/img/imageTwo/home/img4.jpg') no-repeat;
        background-size: cover;
        -webkit-background-size: cover;
        position:absolute;
        top:0;
        left:0;
        .text {
          position:absolute;
          top:35px;
          left:54px;
          .top {
            font-size: 14px;
            color: #fff;
            margin-bottom: 28px;
            .big {
              font-size: 36px;
            }
          }
          .bottom {
            font-size: 20px;
            color: #142436;
            line-height: 36px;
          }
        }
      }
      .productList {
        margin-left: 300px;
        background-color:#fff;
        .item {
          cursor: pointer;
          .ww {
            padding:34px 0 32px 48px;
          }
          .title {
            font-size: 14px;
            color: #b0b3b4;
            padding-bottom:24px;
            .titlename {
              display: inline-block;
              width: 200px;
              vertical-align: middle;
            }
            .subTitle {
              display: inline-block;
              height: 25px;
              line-height: 25px;
              padding:0 10px;
              border: 1px solid #c9c9c9;
              border-radius: 12.5px;
              -webkit-border-radius: 12.5px;
              vertical-align: middle;
            }
          }
          .dates {
            display: inline-block;
            margin-right: 66px;
            .num {
              color: $high;
              font-size: 26px;
              .little {
                font-size: 18px;
              }
            }
            .numTwo {
              font-size: 20px;
              color: #394043;
              .unite {
                font-size: 14px;
                color: #888c8e;
              }
            }
            .type {
              font-size: 14px;
              color: #888c8e;
            }
            .process {
              width:140px;
              height: 4px;
              background-color: #ccc;
              position:relative;
              .inner {
                // width:60px;
                height: 4px;
                background:linear-gradient( left,#ffcc00,#fc7800);
                background:-o-linear-gradient( left,#ffcc00,#fc7800);
                background:-webkit-linear-gradient( left,#ffcc00,#fc7800);
                background:-moz-linear-gradient( left,#ffcc00,#fc7800);
                position:absolute;
                left:0;
                top:0;
              }
            }
          }
          .datesOne {
            width:200px;
            position:relative;
            margin:0;
            .icon {
              width:43px;
              height: 21px;
              background: url('/img/imageTwo/home/icon_home12.png') no-repeat center;
              position:absolute;
              top:-14px;
              right: 38px;
            }
          }
          .anniu {
            display: inline-block;
            .btn {
              padding: 0;
              font-size: 14px;
              width:108px;
              height: 30px;
              line-height: 30px;
              text-align: center;
              color: $high;
              border: 1px solid $high;
              background:none;
              border-radius: 2px;
              -webkit-border-radius: 2px;
              // &:hover {
              //   background:$high;
              //   color: #fff;
              // }
              &.fulled {
                color: #b0b3b4;
                background-color: #FFF;
                border: 1px solid #b0b3b4;
              }
              &.repayment {
                color: #fff;
                background-color: #b0b3b4;
                border: 1px solid #b0b3b4;
              }
            }
          }
          &:not(:last-child) {
            border-bottom: 1px dashed #f0f0f0;
          }
          &:hover {
            box-shadow:0 0 10px rgba(0,0,0,.2);
            .anniu {
              .btn {
                background:$high;
                color: #fff;
              }
            }
          }
        }
      }
    }
  }
  .partSix {
    height: 311px;
    background:url('/img/imageTwo/home/img5.jpg') no-repeat center;
    .stepList {
      font-size:0;
      -webkit-text-size-adjust:none;
      .step {
        display: inline-block;
        width:25%;
        color: rgba(255,255,255,.8);
        text-align: center;
        font-size: 22px;
        padding-top: 50px;
        .icon {
          display: inline-block;
          width: 58px;
          height: 52px;
        }
      }
      .step1 {
        .icon {
          background:url('/img/imageTwo/home/icon_home13.png') no-repeat center;
        }
      }
      .step2 {
        .icon {
          background:url('/img/imageTwo/home/icon_home14.png') no-repeat center;
        }
      }
      .step3 {
        .icon {
          background:url('/img/imageTwo/home/icon_home16.png') no-repeat center;
        }
      }
      .step4 {
        .icon {
          background:url('/img/imageTwo/home/icon_home15.png') no-repeat center;
        }
      }
    }
    .apply {
      text-align: center;
      padding-top: 46px;
    }
    .btn {
      font-size: 22px;
      color: #afb3b4;
      width: 158px;
      height: 52px;
      background-color: #FFF;
      border-radius:52px;
      border: none;
      cursor: pointer;
      &:hover {
        border: 1px solid $high;
        color: $high;
      }
    }
  }
  .partSeven {
    height: 136px;
    overflow: hidden;
    zoom:1;
    padding:55px 0;
    background-color: #FFF;
    .credential {
      width:362px;
      height: 136px;
      float: left;
      cursor: pointer;
      a {
        width:100;
        height: 100%;
        display: block;
      }
    }
    .one {
      background: url('/img/imageTwo/home/zhengshu1.png') no-repeat center;
      &:hover {
        background: url('/img/imageTwo/home/zhengshu11.png') no-repeat center;
      }
    }
    .two {
      margin-left:56px;
      background: url('/img/imageTwo/home/zhengshu2.png') no-repeat center;
      &:hover {
        background: url('/img/imageTwo/home/zhengshu22.png') no-repeat center;
      }
    }
    .three {
      float:right;
      background: url('/img/imageTwo/home/zhengshu3.png') no-repeat center;
      &:hover {
        background: url('/img/imageTwo/home/zhengshu33.png') no-repeat center;
      }
    }
  }
  .partEight {
    overflow: hidden;
    zoom:1;
    .topNav {
      overflow: hidden;
      zoom:1;
      font-size: 14px;
      padding:1rem 0 0.5rem;
      .typeName {
        float: left;
        color: #b0b3b4;
      }
      .more {
        float: right;
        color: #288bff;
      }
    }
    .leftNews {
      width: 670px;
      float: left;
      .newsContent {
        height: 372px;
        .topContent {
          height: 162px;
          position:relative;
          .newsImg {
            width:257px;
            height: 100%;
            position:absolute;
            left: 0;
            top:0;
            overflow: hidden;
            img {
              width: 100%;
            }
            .bestNews {
              width: 39px;
              height: 21px;
              background: url('/img/imageTwo/home/bestNews.png') no-repeat center;
              position:absolute;
              top:0;
              left:18px;
            }
          }
          .rightInfo {
            height: 100%;
            margin-left: 257px;
            padding:30px 44px 20px;
            background-color: #FFF;
            box-sizing:border-box;
            -webkit-box-sizing:border-box;
            .newsTitle {
              font-size: 22px;
              color: #394043;
            }
            .newsText {
              font-size: 12px;
              line-height: 22px;
              padding-top:10px;
              color: #b0b3b4;
            }
          }
        }
        .bottomContent {
          height: 190px;
          padding:21px 34px;
          background-color: #FFF;
          margin-top: 22px;
          box-sizing:border-box;
          .bottomListRoll {
            height: 148px;
            overflow: hidden;
          }
          .bottomList {
            overflow: auto;
            .item {
              font-size: 14px;
              overflow: hidden;
              zoom:1;
              padding:9px 0;
              cursor: pointer;
              .newsHead {
                float: left;
                color: #394043;
              }
              .time {
                float: right;
                color: #b0b3b4;
              }
              &:hover {
                .newsHead,.time {
                  color: $high;
                }
              }
            }
          }
        }
      }
    }
    .rightNotice {
      width: 470px;
      float: right;
      .noticeContent {
        height: 372px;
        padding:36px 28px;
        box-sizing:border-box;
        -webkit-box-sizing:border-box;
        background-color: #FFF;
        .noticeListRoll {
          height: 300px;
          padding-left:10px;
          overflow: hidden;
        }
        .noticeList {
          // padding: 18px 0;
          border-left: 1px dashed #ccc;
          .item {
            height: 30px;
            padding-left:25px;
            line-height: 30px;
            font-size: 14px;
            color: #888c8e;
            position:relative;
            cursor: pointer;
            .title {
              color: #394043;
              float: left;
            }
            .time {
              float: right;
            }
            .circle {
              width:17px;
              height: 17px;
              background-color: #fff;
              border: 1px solid #fff;
              border-radius: 50%;
              -webkit-border-radius: 50%;
              position: absolute;
              left:0;
              top:50%;
              transform:translate(-50%,-50%);
              -webkit-transform:translate(-50%,-50%);
              .inner {
                width:8px;
                height: 8px;
                background-color: #d7d9d9;
                border-radius: 50%;
                -webkit-border-radius: 50%;
                position:absolute;
                top:50%;
                left:50%;
                transform:translate(-50%,-50%);
                -webkit-transform:translate(-50%,-50%);
              }
            }
            &:hover {
              color: $high;
              .title {
                color: $high;
              }
              .circle {
                border: 1px solid $high;
                .inner {
                  background-color: $high;
                }
              }
            }
          }
        }
      }
    }
  }
  .partNine {
    .topNav {
      margin:0;
      padding:36px 0 20px;
      border-bottom: 1px solid #f0f0f0;
      .left {
        color: #b0b3b4;
        font-size: 14px;
      }
    }
    .listWrap {
      position:relative;
      margin: 28px 0 50px;
    }
    .listTwo {
      width:1000px;
      height: 130px;
      margin: 0 auto;
      padding:0;
      .itemTwo {
        width: 190px;
        height: 60px;
        border: 1px solid #f3f3f3;
        float: left;
        cursor: pointer;
        a {
          display: block;
          height: 100%;
        }
        &:hover {
          border: 1px solid $high;
        }
      }
      .index0,.index1,.index2,.index3,.index4,.index5{
        margin-bottom: 10px;
      }
      .index1,.index2,.index3,.index4,.index6,.index7,.index8,.index9 {
        margin-left: 10px;
      }
      .index0 {
        background: url('/img/imageTwo/home/partner0.png') no-repeat center;
        &:hover {
          background: url('/img/imageTwo/home/partner00.png') no-repeat center;
        }
      }
      .index1 {
        background: url('/img/imageTwo/home/partner1.png') no-repeat center;
        &:hover {
          background: url('/img/imageTwo/home/partner11.png') no-repeat center;
        }
      }
      .index2 {
        background: url('/img/imageTwo/home/partner2.png') no-repeat center;
        &:hover {
          background: url('/img/imageTwo/home/partner22.png') no-repeat center;
        }
      }
      .index3 {
