.crowd_details{
  .details_title{
    height: 68px;
    line-height: 68px;
    font-size: 14px;
    color: #b0b3b4;
    a:hover{
      color: #ff6600;
    }
  }
  .details_prop{
    background: #fff;
    overflow: hidden;
    zoom:1;
  }
  .details{
    position: relative;
    .details_left{
      display: inline-block;
      vertical-align: top;
    }
    .plan_right{
      width: 502px;
      display: inline-block;
      vertical-align: top;
      float: right;
      margin:20px 9px 0 0;
      position: absolute;
      right:28px;
      top:0;
      .plan_title{
        font-size: 22px;
        color: #394043;
        font-weight: bold;
      }
      .raise{
        overflow: hidden;
        zoom:1;
        margin-top: 15px;
        font-size: 14px;
        color: #888c8e;
        .raise_left{
          float: left;
        }
        .raise_right{
          float: right;
          .raise_teshu{
            font-size: 20px;
            color: #394043;
          }
        }
      }
      .plan_item{
        height: 48px;
        font-size: 12px;
        color: #b0b3b4;
        word-wrap: break-word;
        word-break: break-all;
        overflow: hidden;
        margin-top: 15px;
      }
      .plan_item2{
        margin-top:10px;
      }
      .bar {
        padding: 8px 0;
        margin-top: 7px;
        .container {
          height: 4px;
          border-radius: 2px;
          background: #e8e8e8;

          .completed {
            width: 0;
            height: 4px;
            border-radius: 2px;
            background: -webkit-linear-gradient(left, #ffce00,#fc7b00);
            background: -o-linear-gradient(left, #ffce00,#fc7b00);
            background: -moz-linear-gradient(left, #ffce00,#fc7b00);
            background: linear-gradient(left,#ffce00,#fc7b00);
            transition:width 1s;
            -moz-transition:width 1s; /* Firefox 4 */
            -webkit-transition:width 1s; /* Safari and Chrome */
            -o-transition:width 1s; /* Opera */
          }
        }
      }
      .target{
        margin-top: 15px;
        .target_money,.target_time{
          display: inline-block;
          font-size: 14px;
          color: #888c8e;
          .heise{
            font-size: 14px;
            color: #394043;
          }
          .heise2{
            color: #ff6600;
          }
          .over{
            display: inline-block;
            font-size: 14px;
            color: #888c8e;
          }
          .over2{
            font-size: 12px;
          }
        }
        .target_money2{
          color: #ff6600;
        }
        .target_time{
          margin-left: 10px;
        }
        .overTime{
          float: right;
        }
      }
      .target2{
      }
      .suport{
        font-size: 12px;
        color: #ffc952;
        margin-top: 22px;
      }
      .prop{
        margin-top: 10px;
        .prop_caption{
          .item,.filed{
            font-size: 14px;
            color: #888c8e;
            .small{
              color: #394043;
            }
            .muji{
              color: #ff6600;
              font-size: 12px;
            }
          }
          .muji{
            color: #ff6600;
            font-size: 12px;
          }
          .over2{
            font-size: 12px;
          }
        }
        .prop_caption2{
          display: inline-block;
        }
        .prop_caption3{
          float: right;
        }
        .prop_caption4{
            margin-top: 10px;
        }
      }
      .plan_info{
        font-size: 14px;
        color: #888c8e;
        margin-top: 30px;
      }
      .plan_but{
        width: 108px;
        height: 30px;
        border: 0;
        outline: none;
        background: #fff;
        border: 1px solid #ff6f20;
        font-size: 18px;
        color: #ff6f20;
        border-radius: 2px;
        float: right;
        margin-top: -20px;
        &:hover{
          background:#ff6f20;
          color: #fff;
        }
        &.huise{
          background: #ccc;
          color: #fff;
          border: 1px solid #ccc;
          cursor: not-allowed;
        }
      }
    }
  }
  .detailsA{
    height: 297px;
  }
  .details_tab{
    .tab_title{
      overflow: hidden;
      zoom:1;
      padding:0;
      margin: 0;
      .item{
        width: 160px;
        height: 55px;
        line-height: 55px;
        text-align: center;
        float: left;
        color: #b0b3b4;
        font-size: 18px;
        cursor: pointer;
        &.selected{
          border-bottom: 3px solid #ff6f20;
          color: #ff6f20;
        }
      }
    }
    .tab_content{
      width: 1150px;
      margin: 0;
      padding:0;
      .item{
        background: #fff;
        border-top: 1px solid #e2e2e2;
      }
      .tab_left{
        width: 785px;
        min-height: 300px;
        background: #fff;
        display: inline-block;
        vertical-align: top;
      }
      .tab_right{
        display: inline-block;
        vertical-align: top;
        margin-left: 38px;
        &.disabled{
          display: none;
        }
        .tab_right_caption{
          width: 320px;
          height: 330px;
          background: #fff;
          margin-top: 20px;
          &:first-child{
            margin-top:0;
          }
          .right_top{
            border-bottom: 1px dashed #e2e2e2;
          }
          .right_title{
            height: 60px;
            line-height: 60px;
            padding: 0 35px;
            .title_left{
              display: inline-block;
              font-size: 20px;
              color: #ff6f20;
              float: left;
            }
            .title_right{
              font-size: 14px;
              color: #394043;
              display: inline-block;
              float: right;
            }
          }
          .right_content{
            padding: 20px 30px 50px;
            .quota{
              font-size: 14px;
              color: #888c8e;
              .quota_teshu{
                color: #394043;
              }
            }
            .quota_info{
              font-size: 12px;
              color: #b0b3b4;
              margin-top: 30px;
              word-wrap:break-word; 
              word-break:break-all; 
              overflow: hidden;
            }
            .quota_but{
              width: 135px;
              height: 40px;
              border: 1px solid #ff6f20;
              font-size: 18px;
              color: #ff6f20;
              background: #fff;
              margin-top: 25px;
              border-radius: 2px;
              &:hover{
                background: #ff6f20;
                color: #fff;
              }
              &.disabled{
                background-color: #ececec;
                border: none;
              }
            }
          }
        }
      }
      .item{
        height: 100%;
        .tab_detail{
          padding: 10px 40px;
          word-wrap:break-word; 
          word-break:break-all; 
          overflow: hidden;
          .row{
            font-size: 16px;
            color: #394043;
            margin-top: 40px;
            .row_item2{
              margin-top: 10px;
            }
            .details_icon{
              width: 17px;
              height: 17px;
              vertical-align: middle;
              position: relative;
              top: 2px;
            }
            .details_icon2{
              width: 17px;
              height: 17px;
              vertical-align: middle;
              position: relative;
              top: 2px;
              left: -6px;
            }
            .explain{
              margin-top: 20px;
              .one{
                font-size: 14px;
                color: #394043;
              }
              .two{
                font-size: 14px;
                color: #b0b3b4;
                line-height: 25px;
              }
            }
            .explain2{
              margin-top: 10px;
            }
            .jiekuan{
              a{
                font-size: 14px;
                color: #378aff
              }
            }
            .risk{
              padding:0;
              margin: 0;
              overflow: hidden;
              zoom:1;
            }
            .risk_item{
              margin-top: 30px;
              display: inline-block;
              float: left;
              .icon{
                display: block;
                margin: 0 auto;
              }
              .risk_info{
                font-size: 14px;
                color: #b0b3b4;
                text-align: center;
              }
            }
            .item1{
              margin-left: 90px;
            }
            .item2{
              margin-left: 40px;
            }
            .ensure,.process{
              text-align: center;
              margin-top: 10px;
            }
            .problem{
              font-size: 18px;
              color: #394043;
            }
            .problem2{
              position: relative;
              font-size: 14px;
              color: #b0b3b4;
            }
          }
          .row1{
            margin-top: 20px;
          }
        }
        .log-list {
            width: 95%;
            margin: 0 auto;
            thead {
                background: #ececec;
                th {
                    height: 45px;
                    text-align: center;
                    font-size: 16px;
                    color: #394043;
                }
            }
            tbody {
                tr {
                    height: 45px;
                    border-bottom: 1px dashed #e2e2e2;
                    font-size: 12px;
                    color: #b0b3b4;
                }
                td {
                    text-align: center;
                }
            }
        }
      }
      .itemrisk3{
        padding: 30px 0;
        overflow: hidden;
        zoom:1;
        // display: none;
        .pagination{
          display: inline-block;
          float: right;
          margin: 40px 30px 0 0;
        }
      }
      .riskOne{
        display: none;
      }
      .riskTwo{
        display: none;
      }
      .itemrisk4{
        display: none;
      }
    }
  }
}
