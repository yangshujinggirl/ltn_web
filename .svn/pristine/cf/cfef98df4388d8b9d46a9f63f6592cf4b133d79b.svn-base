.safe-section{
  width: 100%;
  height: 260px;
  background: #fff;
  .wrap{
    width: 1190px;
    padding: 0;
    overflow: visible;
  }
  .button{
    position: fixed;
    margin-left: 75px;
    margin-top: 0;
    display: inline-block;
    left: -115px;
    bottom: -12px;
    z-index: 99;
    .list{
      display: inline-block;

      li{
        width: 70px;
        height: 44px;
        line-height: 40px;
        text-align: center;
        font-size: 14px;
        background: #ff6f20;
        margin-top: 7px;
        word-wrap: break-word;

        a{
          color: #fff;
        }
      }
    }
  }
  .section{
    width: 996px;
    position: absolute;
    left: 105px;
    top: 0;
    margin: 0 auto;

    .limit{
      margin-top: 40px;
      width: 24%;
      height: 166px;
      display: inline-block;
      text-align: center;
      vertical-align: top;
      .icon1,.icon2,.icon3,.icon4{
        width: 137px;
        height: 137px;
        border-radius: 50%;
        margin: 0 auto;
        // display: inline-block;
        margin-bottom: 10px;
      }
      .text{
        vertical-align: top;
        // margin: -117px 11px 0 105px;
        .title{
          font-size: 18px;
        }
        .item{
          font-size: 14px;
          color: #6a6a6a;
        }
      }

    }
    .limit-tow{
      vertical-align: top;
    }

  }

}
