.account-detailprofit .detailmoney-content {
  padding:10px 35px 35px ;
  p{
    margin:0;
    padding:0;
  }
  .itemnav {
    font-size: 14px;
    font-weight: 700;
    overflow: hidden;
    border-bottom: 2px solid #e2e2e2;
    .itemnav-left {
      width: 100px;
      height: 50px;
      text-align: center;
      line-height: 76px;
      float: left;
      border-bottom: 3px solid #ea5504;
      color: #ea5504;
    }
    .itemnv-right {
      float: right;
      padding-left: 107px;
      height: 50px;
      line-height: 60px;
       background:url(/html/img/bank-ld.png) no-repeat left center;
    }
  }
  .headeramcount {
    font-size: 14px;
    overflow: hidden;
    .remain {
       margin-bottom: 45px;
       text-align: center;
       margin-left: 40px;
       float: left;
       .value {
         color: #fc613b;
       }
      .item {
         line-height: 40px;
         a {
           width: 90px;
           display: inline-block;
           margin: 0 4px;
           border-radius: 5px;
           height: 30px;
           line-height: 30px;
         }
         .ain {
           background-color: #fc613b;
           color: #fff;
         }
         .aout {
           border: 1px solid #fc613b;
           color: #fc613b;
         }
         .ainvest {
           border: 1px solid #2079ff;
           color: #2979ff;
         }
      }
    }
    .sum {
       float: right;
       width: 50%;
       border-left:1px solid #e2e2e2;
       padding-left: 50px;
       .value {
         margin-left: 25px;
         color: #fc613b;
       }
       .item {
         height: 40px;
         line-height: 40px;
       }
    }
  }
  .tips {
    padding-left: 20px;
    background-color: #f7f7f7;
    clear: both;
    height: 35px;
    line-height: 35px;
    .tipsinfo {
      background:url(/html/img/icon-prod-notice.png) no-repeat left center;
      padding-left: 30px;
      mark {
       background-color:#f7f7f7;
      }
    }
  }
  .wishprofit {
    font-size: 14px;
    overflow: hidden;
    border-bottom: 2px solid #e2e2e2;
    margin: 20px auto;
   .profitleft {
      float: left;
      border-bottom: 3px solid #ea5504;
      color: #ea5504;
      margin-left: 15px;
      .item {
        line-height: 36px;
      }
   }
    .profitright {
      float: right;
      .profitaction {
        width: 90px;
        height: 36px;
        border:none;
        background-color: #fc613b;
        color: #fff;
        border-radius: 4px;
        margin-left: 10px;
      }
    }
  }
  .form {
    position: relative;
    .row {
      font-size: 14px;
      margin: 25px 0;
      margin-left: 30px;
      input {
      margin: 0 5px;
    }
    .type {
    display: inline;
    .item {
     display: inline;
     margin: 0 10px;
     cursor: pointer;
     padding: 5px 10px;
     &:hover {
       background-color: #fc613b;
       color: #fff;
       padding: 5px 10px;
       border-radius: 4px;
     }
     &.selected {
       background-color: #fc613b;
       color: #fff;
       padding: 5px 10px;
       border-radius: 4px;
     }
    }
    }
    }
    .submit {
      position: absolute;
      right: 6%;
      top: 65%;
      background-color: #fc613b;
      color: #fff;
      padding: 5px 10px;
      border-radius: 4px;
      cursor: pointer;
    }
  }
  table {
   width: 100%;
   margin-top: 30px;
   text-align: left;
   thead {
    tr {
      background-color: #ececec;
      th {
        font-size: 16px;
        padding: 10px 20px;
      }
    }
   }
   tbody {
    tr {
     &:nth-child(even) {
       background-color: #f7f7f7;
     }
     td {
       padding: 10px 20px;
       font-size: 14px;
     }
    }
   }
  }
  .nolist {
    display: none;
    width: 196px;
    height: 179px;
    margin: 0 auto;
    background: url(/html/img/jz.jpg);
  }
  .earnContent {
    height: 500px;
    margin-top: -90px;
    .leftChart {
      height: 100%;
    }
    .earnings {
      font-size: 12px;
      color:#ea5504;
      text-align: center;
      position: absolute;
      top:233px;
      left:165px;
    }
    .icon {
      width: 16px;
      height: 16px;
      background: url(/html/img/detailprofit_icon1.png);
      position: absolute;
      right: 0;
      top:50%;
      margin-top:-8px;
      cursor: pointer;
      &:hover {
        background: url(/html/img/detailprofit_icon2.png);
      }
    }
    .hint {
      width: 214px;
      height: 48px;
      font-size: 12px;
      padding-top:10px;
      line-height: 17px;
      color:#ea5504;
      text-align: center;
      position: absolute;
      box-sizing:border-box;
      -webkit-box-sizing:border-box;
      display: none;
      &.selected {
        display: block;
      }
    }
    .chartOne {
      width:42%;
      height: 100%;
      position: relative;
      float: left;
      .leftChart {
        width:100%;
      }
      .amount {
        position: absolute;
        right: -36px;
        bottom:40px;
        z-index: 99;
        .fixed , .award {
          font-size: 14px;
          position: relative;
          padding:0 20px;
          height: 60px;
          line-height: 60px;
          &:before {
            content:'';
            display: block;
            width: 10px;
            height: 10px;
            border-radius:50%;
            -webkit-border-radius:50%;
            position: absolute;
            left:0;
            top:50%;
            margin-top: -5px;
          }
        }
        .award {
          .hint {
            background: url(/html/img/detailprofit_04.png) no-repeat center;
            right: -83px;
          }
          &:before {
            background: #e08bff;
          }
        }
        .fixed {
          .hint {
            background: url(/html/img/detailprofit_03.png) no-repeat center;
            top:-47px;
            right:-115px;
          }
          &:before {
            background: #8aa3ff;
          }
        }
      }
    }
    .chartTwo {
      width:42%;
      height: 100%;
      float: right;
      position: relative;
      left: -168px;
      .leftChart {
        width: 100%;
        float: left;
      }
      .rightInfo {
        position:absolute;
        right:-147px;
        top:119px;
        .item {
          font-size: 14px;
          text-align: left;
          height: 60px;
          line-height: 60px;
          padding:0 20px;
          position:relative;
          &:before {
            content:'';
            display: block;
            width: 10px;
            height: 10px;
            border-radius:50%;
            -webkit-border-radius:50%;
            position: absolute;
            left:0;
            top:50%;
            margin-top: -5px;
          }
          .hint {
            top:-30px;
            right:-20px;
            background: url(/html/img/detailprofit_05.png) no-repeat center;
          }
        }
        .item:nth-child(1) {
          margin-top: 100px;
           &:before{
            background-color: #ff4700;
          }
        }
        .item:nth-child(2) {
           &:before{
            background-color: #2fecb8;
          }
        }
        .item:nth-child(3) {
           &:before{
            background-color: #00bdff;
          }
        }
        .item:nth-child(4) {
           &:before{
            background-color: #ff2173;
          }
        }
      }
    }
  }
}