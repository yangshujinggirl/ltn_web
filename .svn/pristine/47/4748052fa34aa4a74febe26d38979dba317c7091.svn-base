                    }
                }
            }
            .accept {
                margin: 20px 0;
                text-align: center;
                .check,
                .label {
                    cursor: pointer;
                }
                .label {
                    padding: 0 5px;
                }
                .link {
                    color: $link-color;
                    &:hover {
                        color: $link-color-hover;
                    }
                }
            }
            .action {
