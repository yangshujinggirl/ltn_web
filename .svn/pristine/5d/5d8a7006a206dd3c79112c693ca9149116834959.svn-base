.product-desc {
     .wrap {
        padding: 0;
        background: #fff;
        border: 1px solid $border-color;
        margin: 20px auto;
    }
    .floatproject {
        padding:78px 48px;
        color: #888c8e;
        font-size: 14px;
        display: none;
        .timeAxis {
            display: inline-block;
            vertical-align: middle;
            width: 804px;
            .line {
                width: 100%;
                height: 4px;
                background-color: #ffe8dc;
                position:relative;
                .sameSell {
                    text-align: center;
                    position:absolute;
                    top:50%;
                    transform: translateY(-50%);
                    -webkit-transform: translateY(-50%);
                    .value,.key {
                        display: block;
                        font-size: 14px;
                    }
                    .value {
                        color: #3a3a3a;
                    }
                    .circle {
                        width: 15px;
                        height: 15px;
                        background-color: #FFF;
                        border: 3px solid #ffe8dc;
                        border-radius: 50%;
                        margin: 12px auto;
                    }
                }
                .beginSell {
                    left: 40px;
                }
                .beginInit {
                    left:178px;
                }
                .become {
                    left: 532px;
                }
                .predictRepaymen {
                    left: 672px;
                }
            }
        }
        .rightInfo {
            vertical-align: middle;
            display: inline-block;
            margin-left: 48px;
            .time {
                line-height: 26px;
                .timeValue {
                    color: #3a3a3a;
                    padding-left: 10px;
                }
            }
        }
    }
    .banner {
        // height: 185px;
    }
    .short .banner {
        background: url("/html/img/img-prod-banner-short.png") center center no-repeat;
    }
    .first .banner {
        background: url("/html/img/img-prod-banner-first.png") center center no-repeat;
    }
    .tab-list {
        margin: 20px 60px;
        padding: 0 40px;
        height: 40px;
        border-bottom: 2px solid $border-color;
        .item {
            display: inline-block;
            vertical-align: bottom;
            font-size: 18px;
            line-height: 2;
            color: $font-color-title;
            font-weight: bold;
            padding: 0 20px;
            margin: 0 20px;
            height: 100%;
            border-bottom: 2px solid $border-color;
            cursor: pointer;
            &:hover,
            &.selected {
                border-bottom: 2px solid $border-color-hover;
                color: $font-color-hover;
            }
        }
    }
    .content-list {
        margin: 20px 60px;
        padding: 20px 0 144px;
        .nolist {
            display: none;
            width: 196px;
            height: 179px;
            margin: 0 auto;
            text-align: center;
            position: relative;
            background: url("/html/img/wushuju.png?v=20160519") no-repeat;
            .logoIcon {
                position: absolute;
                left: -22px;
                top: 130px;
                font-size: 16px;
                .denglu {
                    color: #ff5a00;
                }
                .denglu:hover {
                    color: #6e56ff;
                }
            }
        }
        .item {
            text-align: center;
            .row {
                margin: 0;
                padding: 0 60px;
                line-height: 40px;
                display: block;
                text-align: justify;
                .caption {
                    font-size: 18px;
                    color: $font-color-title;
                    // display: inline-block;
                    // width: 100px;
                    &.sub {
                        font-size: 14px;
                        vertical-align: top;
                    }
                    &.solo {
                        display: block;
                        width: 100%;
                    }
                }
                .field {
                    width: 848px;
                    font-size: 14px;
                    display: inline-block;
                }
                .list,
                .risk {
                    margin: 0;
                    padding: 0;
                    width: 947px;
                    .item {
                        font-size: 14px;
                        display: inline-block;
                        margin-right: 55px;
                        .key,
                        .value {
                            display: block;
                        }
                        .icon1 {
                            width: 74px;
                            height: 86px;
                            cursor: pointer;
                            background: url("/html/img/icon-prod-first-risk-01.png");
                        }
                        .icon2 {
                            width: 84px;
                            height: 88px;
                            cursor: pointer;
                            background: url("/html/img/icon-prod-first-risk-02.png");
                        }
                        .icon3 {
                            width: 94px;
                            height: 99px;
                            cursor: pointer;
                            background: url("/html/img/icon-prod-first-risk-03.png");
                        }
                        .icon4 {
                            width: 98px;
                            height: 95px;
                            cursor: pointer;
                            background: url("/html/img/icon-prod-first-risk-04.png");
                        }
                        .icon5 {
                            width: 100px;
                            height: 100px;
                            cursor: pointer;
                            background: url("/html/img/icon-prod-first-risk-05.png");
                        }
                        .icon6 {
                            width: 100px;
                            height: 100px;
                            cursor: pointer;
                            background: url("/html/img/shenfenzheng.png");
                        }
                        .icon7 {
                            width: 100px;
                            height: 100px;
                            cursor: pointer;
                            background: url("/html/img/hukouben.png");
                        }
                        .icon10 {
                            width: 100px;
                            height: 100px;
                            cursor: pointer;
                            background: url("/html/img/chanquandiaocha.png");
                        }
                        .icon11 {
                            width: 100px;
                            height: 100px;
                            cursor: pointer;
                            background: url("/html/img/diyahetong.png");
                        }
                        .icon12{
                            width: 100px;
                            height: 100px;
                            cursor: pointer;
                            background: url(/html/img/cheweihetong.png) no-repeat;
                        }
                        .icon13{
                            width: 100px;
                            height: 100px;
                            cursor: pointer;
                            background: url(/html/img/gongzishouru.png) no-repeat;
                        }
                        .icon14{
                            width: 100px;
                            height: 100px;
                            cursor: pointer;
                            background: url(/html/img/shebaogongjijin.png) no-repeat;
                        }
                        .icon15{
                            width: 100px;
                            height: 100px;
                            cursor: pointer;
                            background: url(/html/img/fangchanzhengming.png) no-repeat;
                        }
                        .icon16{
                            width: 100px;
                            height: 100px;
                            cursor: pointer;
                            background: url(/html/img/fuwuhetong.png) no-repeat;
                        }
                        .icon17{
                            width: 100px;
                            height: 100px;
                            cursor: pointer;
                            background: url(/html/img/zhuangxieshuoming.png) no-repeat;
                        }
                    }
                }
                #lechaodai{
                    display: none;
                }
                #chaozhidai{
                    display: none;
                }
                #yichaodai{
                    display: none;
                }
                .risk {
                    padding: 20px 0;
                    .items {
                        height: 30px;
                        line-height: 30px;
                        cursor: pointer;
                        margin-top: 20px;
                        background: rgba(0,0,0,.1)url(/html/img/icon-stat-success.png) center right no-repeat;
                    }
                    .key {
                        padding-left: 10px;
                    }
                }
                #anyidai{
                    display: none;
                    width: 1068px;
                    .item{
                        margin-right:28px;
                        text-align: center;
                        .key{
                            text-indent: -13px;
                        }
                    }
                }
                #liangchaodai{
                    display: none;
                    width: 1068px;
                    .item{
                        margin-right:50px;
                        text-align: center;
                        .key{
                            text-indent: -13px;
                        }
                    }
                }
                #zhanyidai{
                    display: none;
                    width: 1068px;
                    .item{
                        margin-right:28px;
                        text-align: center;
                        .key{
                            text-indent: -13px;
                        }
                    }
                }

                .table {
                    margin-top: 30px;
                    padding: 20px 0;
                    th,
                    td {
                        line-height: 20px;
                        text-align: center;
                        padding: 0 20px 30px 0;
                    }
                    td {
                        width: 10%;
                    }
                    td:nth-child(6),
                    td:nth-child(8) {
                        width: 20%;
                        text-align: left;
                    }
                    th {
                        text-align: center;
                    }
                }
                .bar {
                    padding: 20px 0;
                    .outer {
                        background: #fafafa;
                        border: 1px solid $border-color;
                        height: 35px;
                        position: relative;
                        .inner {
                            height: 37px;
                            background: #febb92;
                            position: absolute;
                            top: -1px;
                            left: -1px;
                        }
                    }
                    .label {
                        font-size: 14px;
                    }
                }
                .assurance {
                    // background: url("/html/img/img-prod-desc-assurance.png") center center no-repeat;
                    // width: 100%;
                    // height: 490px;
                    img {
                      width: 100%;
                    }
                }
                .business {
                    background: url("/html/img/img-prod-desc-business.png") center center no-repeat;
                    width: 100%;
                    height: 250px;
                }
                .log-list {
                    width: 100%;
                    thead {
                        background: #ececec;
                        th {
                            text-align: center;
                        }
                    }
                    tbody {
                        tr:nth-of-type(even) {
                            background: #f7f7f7;
                        }
                        td {
                            text-align: center;
                        }
                    }
                }
                 .product-page {
                   margin: 0;
                   .pages {
                     margin: 20px 0;
                   }
                 }
            }
            .dash {
                height: 0;
                border: none;
                border-top: 1px dashed $border-color;
            }
            >.detail,
            >.risk,
            >.log,
            >.qa {
                display: none;
            }
            .lable-list {
              border-bottom: 1px dashed #ccc;
              .lable-item {
                text-align: left;
                overflow: hidden;
                zoom:1;
                margin-bottom: 30px;

                .lable-type,.lable-desc {
                  display: inline-block;
                  vertical-align: top;
                }
                .lable-type {
                  font-size: 18px;
                  margin: 0;
                }
                .lable-desc {
                  font-size: 14px;
                  width: 90%;
                  text-align: left;
                  margin: 0;
                  margin-left: 10px;
                }
              }
            }
            .fk-ensure {
              text-align: left;
              .fk {
                font-size: 18px;
              }
              .img-list {
                overflow: hidden;
                zoom:1;
                .img-item {
                  text-align: center;
                  float: left;
                  &:not(:last-child) {
                    margin-right: 6px;
                  }
                }
              }
            }
        }
        &.detail .detail {
            display: block;
        }
        &.risk .risk {
            display: block;
        }
        &.log .log {
            display: block;
        }
        &.qa .qa {
            display: block;
            .title {
                font-size: 18px;
                display: block;
            }
            .caption {
                .icon {
                    width: 52px;
                    height: 43px;
                    line-height: 36px;
                    display: inline-block;
                    position: relative;
                    left: -10px;
                    top: 0;
                    text-align: center;
                    color: #fff;
                    font-size: 18px;
                    background: url("/html/img/duihuakuang.png") no-repeat;
                }
            }
            .section {
                padding-left: 58px;
                position: relative;
                .field {
                    // padding-left: 50px;
                    display: inline-block;
                    padding-left: 34px;
                }
                .answer {
                    font-size: 18px;
                    font-weight: bold;
                    display: inline-block;
                    position: absolute;
                    top: 13px;
                    left: 54px;
                }
            }
        }
    }
    .cover{
            width:100%;
            height:100%;
            position: fixed;
            left:0;
            top:0;
            z-index: 99;
            display:none;
            background-color:rgba(0, 0, 0,0.5);
        }
    .lunbo{
        position: fixed;
        left:0;
        top:0;
        z-index: 99999;
        text-align:center;
        display:none;
        width: 100%;
        .swiper-container{
            height:1000px;
            overflow: hidden;
            width:100%;
            z-index: 9999;
            .swiper-wrapper{
                height:1000px;
                img{
                    max-width:100%;
                    _width:expression(this.width > 100% ? "100%" : this.width);
                    max-height:620px;
                    _width:expression(this.width > 620 ? "620px" : this.width);
                }
            }
        }
    }
    .close{
        width:41px;
        height:41px;
        position: fixed;
        right:10px;
        top:10px;
        z-index: 1111111;
        display:none;
        cursor: pointer;
    }
    .LCTtankuang{
        width: 470px;
        height: 260px;
        background: #fff;
        position: fixed;
        left: 50%;
        top: 50%;
        margin-left: -235px;
        margin-top: -130px;
        display: none;
        z-index: 999;
        .tkTitle{
            font-size: 14px;
            color: #ff5900;
            border-bottom: 2px solid #e2e2e2;
            .tishi2{
                width: 120px;
                height: 55px;
                line-height: 55px;
                text-align: center;
                font-weight: bold;
                display: inline-block;
                border-bottom: 3px solid #ff5900;
            }
        }
        .tkCont{
            width: 310px;
            margin: 0 auto;
            text-align: center;
            font-size: 22px;
            color: #ff0000;
            margin-top: 45px;
        }
        .tkCont2{
            margin-top: 10px;
        }
        .anniu{
            margin: 0 auto;
        }
        .tkbut{
            width: 140px;
            height: 40px;
            display: block;
            margin: 25px auto;
            font-size: 18px;
            outline: none;
            border: 0;
            background: #ff0000;
            color: #fff;
        }
        .but4{
            margin: 10px auto;
        }
        .close2{
            width: 17px;
            height: 17px;
            position: absolute;
            right: 20px;
            top: 25px;
            cursor: pointer;
        }
    }
    .long .content-list .detail .risk {
        .iconUrl {
            background: gray center center no-repeat;
        }
    }
}
