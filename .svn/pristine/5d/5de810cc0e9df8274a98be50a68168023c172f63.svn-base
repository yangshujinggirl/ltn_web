.account-bird {
  $high:#ff5900;
  padding:10px 35px 35px ;
  .itemnav {
    font-size: 14px;
    font-weight: 700;
    height: 50px;
    border-bottom: 1px solid #e2e2e2;
    .itemnav-left {
      text-align: center;
      float: left;
      .ite {
        width: 100px;
        height: 50px;
        display: inline-block;
        line-height: 60px;
        color: #333;
        position: relative;
        bottom:-1px;
        &.selected {
          border-bottom: 3px solid $high;
          color: $high;
        }
      }
    }

    .itemnv-right {
      float: right;
      font-size: 14px;
      padding-left: 124px;
      height: 50px;
      color: #999;
      line-height: 60px;
      background:url(/img/viewall_liandong.png) no-repeat left center;
    }
  }
  .bird_cont {
    width: 873px;
    height: 157px;
    margin: 30px 0;
    background: #f7f7f7;
    padding-left: 30px;
    padding-bottom: 30px;
    .caption {
      width: 185px;
      height: 105px;
      display: inline-block;
      text-align: left;
      font-size: 14px;
      .filed {
        font-size: 16px;
        color: #666;
      }
      .num {
        font-size: 36px;
        color: #ff5900;
        margin-top: 8px;
      }
      .samll {
        font-size: 14px;
      }
      .current {
        color: #999;
      }
      .current2 {
        margin-top: 20px;
      }
      .bird_num {
        color: #666;
        font-size: 16px;
      }
    }
    .caption1, .caption2 {
      position: relative;
      top: -15px;
    }
    .caption3 {
      margin-left: 30px;
    }
    hr {
      height: 126px;
      border: 0;
      display: inline-block;
      border: 1px dashed #e2e2e2;
      position: relative;
      left: -31px;
      top: 28px;
    }
  }
  .more {
    .header {
      font-size: 14px;
    }
    .mored_cont {
      margin-top: 20px;
    }
    .more_box1 {
      width: 209px;
      height: 125px;
      border: 1px solid #ebebeb;
      display: inline-block;
      margin-left: 16px;
      text-align: center;
      cursor: pointer;
      border-radius: 4px;
      &.bg {
        -moz-box-shadow: 5px 5px;
        -webkit-box-shadow: 5px 5px;
        box-shadow: 0 0 8px 5px #f3f3f3;
      }
      .more_icon {
        width: 87px;
        height: 64px;
      }
      .inexplicable {
        font-size: 16px;
        color: #999;
        margin-top: 10px;
      }
      .icon1 {
        background: url(/img/more_icon1.png) no-repeat;
        margin: 0 auto;
      }
      .caise1 {
        background: url(/img/bird_icon1.png) no-repeat;
        margin: 0 auto;
      }
      .caise2 {
        background: url(/img/bird_icon2.png) no-repeat;
        margin: 0 auto;
      }
      .caise3 {
        background: url(/img/bird_icon3.png) no-repeat;
        margin: 0 auto;
      }
      .caise4 {
        background: url(/img/bird_icon4.png) no-repeat;
        margin: 0 auto;
      }
      .icon2 {
        background: url(/img/more_icon2.png) no-repeat;
        margin: 0 auto;
      }
      .icon3 {
        background: url(/img/more_icon3.png) no-repeat;
        margin: 0 auto;
      }
      .icon4 {
        background: url(/img/more_icon4.png) no-repeat;
        margin: 0 auto;
      }
    }
    .box1 {
      margin-left: 0;
    }
    .hints {
      font-size: 12px;
      color: #333;
      margin-top: 30px;
      .hints_icon {
        width: 32px;
        height: 32px;
        background: url(/img/more_laba.png) no-repeat;
        display: inline-block;
        position: relative;
        top: -10px;
      }
      .significance {
        display: inline-block;
        line-height: 25px;
        margin-left: 15px;
      }
    }
  }
  .birdcontent {
    margin-top: 30px;
    width: 950px;
    .itemnav {
      font-size: 20px;
      overflow: hidden;
      margin-bottom: 10px;
      margin-left: -8px;
      .item {
        width: 140px;
        height: 40px;
        line-height: 40px;
        font-size: 16px;
        float: left;
        text-align: center;
        margin: 0 10px;
        border: 1px solid #fff;
        &:hover, &.selected {
          border: 1px solid #ff5900;
          border-radius: 20px;
          color: #ff5900;
        }
      }
    }
    .nolist {
      display: none;
      width: 196px;
      height: 179px;
      margin: 0 auto;
      background: url(/img/jz.jpg);
    }
    tr {
      font-size: 14px;
      height: 60px;
      border-bottom: 1px dashed #e2e2e2;
      td, th {
        padding: 10px 5px;
        text-align: left;
        text-indent: 10px;
        width: 13%;
        &.productname {
          width: 14%;
        }
        &.protocol a {
          color: #38a2ff;
        }
      }
      td {
        color: #999;
      }
      /* &:nth-child(even) {
        background-color: #f7f7f7;
      } */
    }
    table {
      margin: 0 -21px;
      width: 100%;
      thead tr {
        font-size: 16px;
        background-color: $acount-myinvest-bgcolor;
        border: 0;
      }
    }
    #contenttableBird .item {
      display: none;
    }
    .detailhuode #detailhuode {
      display: block;
    }
    .detailhuode {
      padding: 0 30px;
      margin-left: -8px;
    }
    .detailshiyong {
      padding: 0 30px;
      margin-left: -8px;
    }
    .detailshiyong #detailshiyong {
      display: block;
    }
  }
}