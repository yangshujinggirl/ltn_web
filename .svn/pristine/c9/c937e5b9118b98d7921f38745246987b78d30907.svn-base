.address_account{
  padding: 35px 35px;
  .viewall_top{
    width: 938px;
    position: relative;
    left: -13px;
    font-size: 14px;
    height: 42px;
    border-bottom: 1px solid #e2e2e2;
    .viewall_left{
      width: 100px;
      color: #ff5900;
      font-weight: bold;
      border-bottom: 3px solid #ff5900;
      display: inline-block;
      float: left;
      text-align: center;
      line-height: 42px;
    }
    .viewall_right{
      display: inline-block;
      float: right;
      color: #999999;
    }
    .liandong_icon{
      width: 124px;
      height: 42px;
      display: inline-block;
      vertical-align: middle;
      position: relative;
      left: -5px;
      top: 0;
      background: url(/html/img/viewall_liandong.png) no-repeat;
    }
  }
  .none_address{
    margin-top: 25px;
    position: absolute;
    display: none;
    span{
      font-size: 14px;
      color: #888c8e;
    }
    button{
      width: 108px;
      height: 30px;
      font-size: 14px;
      border: 1px solid #ff7036;
      border-radius: 3px;
      background: #fff;
      text-align: center;
      line-height: 27px;
      color: #ff7036;
    }
  }
 .form {
    font-size: 14px;
    margin-top: 25px;
    position: relative;
    left: -207px;
    display: none;
    .row {
      line-height: 35px;
      margin: 15px 0;
      position: relative;
      .caption {
        font-size: 14px;
        color: #888c8e;
        text-align: right;
        display: inline-block;
        width: 33.333%;
        box-sizing: border-box;
        padding: 0 20px 0 0;
      }
      .docs-methods{
        position:absolute;
        left: 310px;
        top: -1px;
      }
      &.validate-failed {
          .field {
            position: relative;
              .input {
                  outline: 5px auto #f78e8b;
                  color: #cacaca;
                  outline-offset: -2px;
              }
          }
          .validation {
              display: inline-block;
              position: absolute;
              left: 512px;
              height: 34px;
              background: url("/html/img/icon-mark-warning.png") 10px center no-repeat;
          }
      }
      &.validate-success {
          .validation {
              display: inline-block;
              height: 14px;
              background: url("/html/img/icon-mark-ok.png") 10px center no-repeat;
          }
      }
      .field {
        width: 30%;
        display: inline-block;
        vertical-align: middle;

        .input {
          height: 35px;
          color: #cacaca;
          box-sizing: border-box;
          padding: 0 15px;
          float: left;
          border: 1px solid #e2e2e2;
          border-radius: 4px;

          &.validate {
            width: 60%;
          }
        }
        .validate-image, .validate-sms {
          width: 35%;
          height: 35px;
          float: right;
        }
        .validate-sms {
          border: 1px solid #38a2ff;
          border-radius: 4px;
          box-sizing: border-box;
          background: none;
          font-size:14px;
          color:#38a2ff;
        }
        .check {
          cursor: pointer;
        }
        .info {
          font-size: 12px;
          padding: 0 6px;
          cursor: pointer;
        }
        .action {
          color: #fff;
          background: #ff6f20;
          border: none;
          box-sizing: border-box;
          font-size: 18px;
          height: 40px;
          width: 180px;
          border-radius: 4px;

          &:hover {
            background: #ff5a00;
          }
          &.disabled {
            background: #ccc;
          }
        }
      }

      .validation {
        color: #f78e8b;
        padding: 0 30px;
        width: 33.333%;
        position: relative;
        left: -75px;
        box-sizing: border-box;
        display: inline-block;
        .icon {
          color: #fff;
          display: inline-block;
          text-align: center;
          vertical-align: middle;
          background: #f78e8b;
          width: 14px;
          height: 14px;
          line-height: 1;
          border-radius: 7px;
          margin: 0 5px;
        }
      }

      .tips, .accept, .action {
        padding: 0 120px;
      }
      .tips {
        margin-left: 210px;
        .content {
          font-size: 12px;
          line-height: 24px;
          text-align: justify;
          color: #8a8a8a;
          .mark {
            background: none;
            color: $font-height-light-color;
          }
        }
      }

      .accept {
        margin: 15px 0 ;
        margin-left: 35px;
        text-align: center;
        .check, .label {
          cursor: pointer;
        }
        .label {
          padding: 0 5px;
        }
        .link {
          color: $link-color;
          &:hover {
            color: $link-color-hover;
          }
        }
      }

      .action {
        text-align: center;
        margin-right: 140px;
        .button {
          width: 180px;
          height: 45px;
          margin: 0 30px 0 0;
          border: none;
          border-radius: 4px;
          background: $bg-btn;
          color: #fff;
          font-size: 18px;
          &:hover {
            background: $bg-btn-hover;
          }
          &.disabled{
                background: #d7d7d7;
               cursor: not-allowed;
               color: #fff;
          }
        }
        .link {
          color: $link-color;
          &:hover {
            color: $link-color-hover;
          }
        }
        .text {
          margin: 0 0 0 30px;
        }
      }
       .tips1{
          margin: 10px 0 0 244px;
        }
      }
      .login-tip {
        margin-top: 20px;
        box-sizing: border-box;
        margin-left: 332px;
        color: #f78e8b;
        display: none;
        .icon-wran {
            display: inline-block;
            width: 14px;
            height: 14px;
            background: url("/html/img/icon-mark-warning.png") no-repeat;
            background-size: 14px 14px;
            vertical-align: middle;
        }
        .wran-tip{
            margin-left: 6px;
        }
      }
      .address_details{
        width: 512px;
        height: 164px;
        position: absolute;
        left: 310px;
        border: 1px solid #e2e2e2;
        border-radius: 4px;
        resize: none;
      }
      .address_but{
        position: absolute;
        bottom:-197px;
        left: 310px;
        width: 210px;
        height: 40px;
        border-radius: 3px;
        background:#ff6f20;
        font-size: 18px;
        color: #fff;
        border:0;
        outline: none;
      }
      .address_but2{
        position: absolute;
        bottom:-197px;
        left: 310px;
        width: 210px;
        height: 40px;
        border-radius: 3px;
        background:#ff6f20;
        font-size: 18px;
        color: #fff;
        border:0;
        outline: none;
      }
  }
  .big_box3{
    position: relative;
    display: none;
  }
  .editors{
    width: 920px;
    border: 1px solid #e2e2e2;
    margin-top: 25px;
    display: none;
    .big_box{
      background: #ebebeb;
    }
    .editors_list{
      overflow: hidden;
      zoom:1;
      padding:0;
      margin: 0;
      padding: 10px 20px;
      li{
        float: left;
        font-size: 14px;
        color: #394043;
        word-wrap:break-word;
        word-break:break-all;
        overflow: hidden;
      }
      .recipient{
        width: 100px;
        visibility:visible;
      }
      .locality{
        width: 180px;
        margin-right: 65px;
        visibility:visible;
      }
      .dizhi{
        width: 203px;
        margin-right: 60px;
        visibility:visible;
      }
      .tel{
        width: 159px;
        visibility:visible;
      }
      .handle{
        width: 85px;
        .bianji{
          color: #288bff;
          cursor: pointer;
        }
        .del{
          color: #ff0000;
          cursor: pointer;
        }
      }
    }
  }
}
